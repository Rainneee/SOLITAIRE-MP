<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="gamePane" prefHeight="650.0" prefWidth="900.0" style="-fx-background-color: green;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.GameController">
   <children>
      <Canvas fx:id="gameCanvas" height="400.0" onMouseDragged="#dragCard" onMousePressed="#pickUpCard" onMouseReleased="#dropCard" width="600.0" />
      <Button layoutX="7.0" layoutY="7.0" mnemonicParsing="false" onAction="#Draw" prefHeight="160.0" prefWidth="110.0" style="-fx-background-color: transparent;" />
   </children>
</Pane>
